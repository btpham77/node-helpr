{
  "name": "helpr",
  "version": "0.13.5",
  "description": "",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/tony-kerz/node-helpr.git"
  },
  "scripts": {
    "ava": "cross-env DEBUG=test:*,app:* ava",
    "pretest": "xo",
    "test": "npm run ava",
    "prebuild": "npm run test",
    "build": "babel ./src -d ./dist"
  },
  "author": "tony kerz",
  "license": "ISC",
  "dependencies": {
    "debug": "^2.6.0",
    "fast-safe-stringify": "^1.1.3",
    "jsondiffpatch": "^0.2.4",
    "lodash": "^4.17.4",
    "lru-cache": "^4.0.2",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "ava": "^0.18.1",
    "babel-cli": "^6.22.2",
    "babel-preset-env": "^1.1.8",
    "cross-env": "^3.1.4",
    "test-helpr": "^0.1.10",
    "xo": "^0.17.1"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "rules": {
      "no-console": "error",
      "no-unused-expressions": [
        "error",
        {
          "allowShortCircuit": true,
          "allowTernary": true
        }
      ]
    }
  }
}
